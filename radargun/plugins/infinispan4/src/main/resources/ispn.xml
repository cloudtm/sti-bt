<?xml version="1.0" encoding="UTF-8"?>
<infinispan
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="urn:infinispan:config:5.2 http://www.infinispan.org/schemas/infinispan-config-5.2.xsd"
        xmlns="urn:infinispan:config:5.2">
    <global>
        <globalJmxStatistics
                enabled="true"
                jmxDomain="org.infinispan"/>
        <transport
                clusterName="infinispan-cluster">
            <properties>
                <property
                        name="configurationFile"
                        value="jgroups/jgroups.xml" />
            </properties>
        </transport>
    </global>
    <default>
        <locking
                isolationLevel="SERIALIZABLE"
                concurrencyLevel="32"
                writeSkewCheck="false"
                useLockStriping="false"
                lockAcquisitionTimeout="500"/>
        <transaction
                transactionManagerLookupClass="org.infinispan.transaction.lookup.DummyTransactionManagerLookup"
                useEagerLocking="false"
                transactionMode="TRANSACTIONAL"
                syncRollbackPhase="false"
                cacheStopTimeout="30000"
                useSynchronization="false"
                syncCommitPhase="true"
                lockingMode="OPTIMISTIC"
                eagerLockSingleNode="false"
                use1PcForAutoCommitTransactions="false"
                autoCommit="true" >
                <recovery enabled="false" />
        </transaction>
        <jmxStatistics
                enabled="true"/>
        <deadlockDetection
                enabled="false"/>
        <clustering mode="d">
            <sync
                    replTimeout="60000" />
            <hash
		    factory="org.radargun.cachewrappers.CustomHashingFactory"
		    numSegments="256"
                    numOwners="4"
                    >
                    <groups enabled="true" />
            </hash>
            <l1
                    enabled="false"
                    onRehash="false"
                    lifespan="600000"
                    invalidationThreshold="-1"
                    cleanupTaskFrequency="600000" />
        </clustering>
        <customInterceptors>
				<interceptor
                    after="org.infinispan.interceptors.InvocationContextInterceptor"
                    class="org.infinispan.distribution.wrappers.CustomStatsInterceptor"/>
        </customInterceptors>
        <versioning
                enabled="true"
                versioningScheme="GMU" />
    </default>
    <namedCache
            name="asdkaskd" />
</infinispan>
